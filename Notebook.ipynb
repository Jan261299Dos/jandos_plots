    "import matplotlib.pyplot as plt\n",
    "import glob\n",
    "\n",
    "# Get the paths of all plot files in the 'plots' folder\n",
    "plot_paths = glob.glob('plots/*.png')\n",
    "\n",
    "# Display the plots in the notebook\n",
    "for path in plot_paths:\n",
    "    img = plt.imread(path)\n",
    "    plt.imshow(img)\n",
    "    plt.axis('off')\n",
    "    plt.show()\n",
    "\n"
   ]
  }
